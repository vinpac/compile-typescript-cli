  "author": "Vinicius Pacheco Furtado <vin175pacheco@gmail.com>",
  "description": "Compile a typescript file with webpack",
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "tncc": "./bin/tncc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vinpac/tncc.git"
